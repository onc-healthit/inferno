name: <%= capability_statement.downcase + '_module' %>
title: <%= title %>
description : <%= description %>
fhir_version: r4
default_test_set: ad_hoc_testing
test_sets:
  ad_hoc_testing:
    view: default
    tests:
      - name: MCODE Profiles
        run_all: true
        sequences:<% sequences.each do |sequence| %>
        - <%=sequence[:class_name][0].upcase + sequence[:class_name].slice(1..-1)%><% end %>

